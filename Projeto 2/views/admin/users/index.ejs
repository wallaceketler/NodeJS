<%- include ('../../partials/header.ejs') %>
<%-include ('../../partials/navbar.ejs') %>


<div class="container">
    <hr>
    <h2>Usuários</h2>
    <a class="btn btn-success" href="/admin/user/create">Cadastrar novo usuário</a>
    <hr>
    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>E-mail</th>
            
        
            </tr>
            <% users.forEach((user)=> { %>
                <tr>
                    <th> <%= user.id %> </th>
                    <th> <%= user.email%> </th>

                </tr>  
            <% }) %>
        </thead>
    </table>
</div>


<%-include ('../../partials/footer.ejs')%>